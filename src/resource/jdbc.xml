<?xml version="1.0" encoding="UTF-8"?>
<!-- 参数说明：
	clazz:驱动类
	dbUrl:连接数据库的地址
	user:数据库连接的用户名
	password:数据库连接的密码
	schema:数据库连接的模式
 -->
<!--样例：以hhdb为例-->
<jdbc>
	<parameter key="clazz">org.hhdbsql.Driver</parameter>
	<parameter key="dbUrl">jdbc:hhdbsql://localhost:1432/hhdb</parameter>
	<parameter key="user">dba</parameter>
	<parameter key="password">123456</parameter>
	<parameter key="schema">public</parameter>
</jdbc>

	